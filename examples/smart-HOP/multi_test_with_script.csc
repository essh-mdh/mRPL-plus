  <plugin>
    se.sics.cooja.plugins.ScriptRunner
    <plugin_config>
      <script>TIMEOUT(120000, log.log("Total PRR = " + totalPRR + "Sent/Recv "+ totalSent + "/" + totalReceived+ "\n Average Delay = " +((average_delay/n_hand_offs)-3000)+"\n"));&#xD;
totalPRR = 0;&#xD;
totalReceived = 0;&#xD;
totalSent = 0;&#xD;
start_collecting = 0;&#xD;
hand_off_delay = 0;&#xD;
start_time = 0;&#xD;
end_time = 0;&#xD;
average_delay = 0;&#xD;
n_hand_offs = 0;&#xD;
reached_target_packet = 0;&#xD;
/*Start collecting data only after the first reception*/&#xD;
while(1) &#xD;
{&#xD;
	YIELD();&#xD;
	msgArray = msg.split(' ');&#xD;
	if(msgArray[0].equals("DATA"))&#xD;
    {&#xD;
		if(msgArray[1].equals("recv"))&#xD;
        {&#xD;
		start_collecting = 1;&#xD;
		// Received packet;&#xD;
		totalReceived++;&#xD;
		totalPRR = totalReceived / totalSent;&#xD;
		//log.log("Total PRR " + totalPRR + "Sent/Recv "+ totalSent + "/" + totalReceived+ "\n")&#xD;
		}&#xD;
		&#xD;
		if(msgArray[1].equals("send") &amp;&amp; start_collecting == 1) &#xD;
        {&#xD;
		// Sent packet&#xD;
		totalSent++;&#xD;
            if(msgArray[5].equals("4000"))&#xD;
            {&#xD;
                 log.log("#####################\nTotal PRR = " + totalPRR + "\n Average Delay = " +((average_delay/n_hand_offs)-3000)+"\n#####################");&#xD;
               stopSimulation();&#xD;
            }&#xD;
		//log.log("Sent packet " +totalSent+ "\n")&#xD;
		}&#xD;
	}&#xD;
	if(msgArray[0].equals("Start"))&#xD;
    {&#xD;
	    start_time = time;&#xD;
        //log.log("Start time = " +start_time+ "\n")&#xD;
	}&#xD;
	if(msgArray[0].equals("End"))&#xD;
    {&#xD;
        n_hand_offs++;&#xD;
		end_time = time;&#xD;
		hand_off_delay = end_time - start_time;&#xD;
	    average_delay += hand_off_delay;&#xD;
		log.log("DELAY = " + hand_off_delay + "\n");&#xD;
	    log.log("Total PRR " + totalPRR + "Sent/Recv "+ totalSent + "/" + totalReceived+ "\n");&#xD;
	}&#xD;
}</script>
