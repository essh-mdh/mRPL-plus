      <script>TIMEOUT(120000, log.log("Total PRR " + totalPRR + "\n"));
totalPRR = 0;
totalReceived = 0;
totalSent = 0;
/*Start collecting data only after the first reception*/

while(1) {
YIELD();
msgArray = msg.split(' ');
if(msgArray[0].equals("DATA")) {
if(msgArray[1].equals("recv")) {

    
// Received packet
totalReceived++;
totalPRR = totalReceived / totalSent;
log.log("Total PRR " + totalPRR + "Sent/Recv "+ totalSent + "/" + totalReceived+ "\n")
}

if(msgArray[1].equals("send")) {
// Sent packet
totalSent++;
}
}
}</script>
